{
    "cyberRoyalMode": "pvwa",
    "pvwaUrl": "https://pam.kubi.gg/PasswordVault",
    "pvwaAuthMethod": "Cyberark",
    "pvwaSafeSearch": "PAM",
    "folderCreation": "safeName",

    "pvwaAdditionalProperties": ["location"],
    "connectionDescription": "location",

    "psmRdpAddress": "pam-pm1.kubi.gg",
    "psmSshAddress": "pam-psmp1.kubi.gg",
    "platformMappings": {
        "UnixSSH": {
            "connections": [{
                    "type": "SSH",
                    "components": ["PSMP-SSH"]
                },
                {
                    "type": "SFTP",
                    "components": ["PSMP-SFTP"]
                },
                {
                    "type": "RDP",
                    "components": ["PSM-WinSCP"]
                }
            ]
        },
        "WinDomain": {
            "psmRemoteMachine": 1,
            "connections": [{
                "type": "RDP",
                "components": ["PSM-RDP"]
            }]
        },
        "WinServerLocal": {
            "namePrefix": "Local - ",
            "namePostfix": "",
            "psmRemoteMachine": 0,
            "color": "#FF0000",
            "connections": [{
                "type": "RDP",
                "components": ["PSM-RDP"]
            }]
        }
    }
}